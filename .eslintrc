{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier", "prettier/react"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "prettier/prettier": [
    "error",
    {
      "trailingComa": "es5",
      "singleQuote": "true",
      "printWidth": 120
    }
  ],
  "plugins": ["react", "prettier", "eslint-plugin-prettier"],
  "rules": {}
}
